{
  "name": "edutech-backend",
  "version": "1.0.0",
  "description": "Backend API for Edutech Platform",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register --files src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:setup": "prisma migrate deploy && prisma generate",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "edutech",
    "api",
    "express",
    "typescript"
  ],
  "author": "Edutech Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.0.1",
    "helmet": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.6.0",
    "stripe": "^14.9.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.25",
    "@types/sanitize-html": "^2.16.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
